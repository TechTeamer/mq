{
  "name": "@techteamer/mq",
  "version": "6.3.3-alpha",
  "description": "A RabbitMQ wrapper for node",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "author": "TechTeamer",
  "contributors": [
    "Gábor Tóth <gabor@techteamer.com>",
    "Zoltán Nagy <zoltan.nagy@techteamer.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TechTeamer/mq"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup.sh",
    "lint": "eslint .",
    "unit": "mocha --exit",
    "test": "eslint . && c8 --temp-directory=./test/coverage/tmp mocha --exit && c8 report --reporter=text-lcov --report-dir=./test/coverage/ --reporter=lcovonly  | coveralls"
  },
  "type": "module",
  "dependencies": {
    "amqplib": "^0.10.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "c8": "^9.1.0",
    "chai": "^4.3.7",
    "coveralls": "^3.1.1",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^10.2.0",
    "protobufjs": "^7.2.3",
    "seed-random": "^2.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "peerDependencies": {
    "protobufjs": "^7.2.3"
  }
}
